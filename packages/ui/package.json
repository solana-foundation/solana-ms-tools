{
  "name": "@solana-foundation/ms-tools-ui",
  "version": "0.8.4",
  "description": "Reusable React components built with React, Tailwind CSS, and Shadcn, forming the visual building blocks of microsites",
  "main": ".",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./index.js": "./dist/index.js",
    "./styles.css": "./dist/index.css"
  },
  "scripts": {
    "build": "tsup ./src/index.ts --config=./tsup-esm.config.mjs --tsconfig=./tsconfig.json",
    "dev": "pnpm run storybook",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{js,mjs,cjs,ts,tsx}\" --config ./prettier.config.js",
    "format:fix": "pnpm run format --write",
    "clean": "rm -rf dist",
    "playwright:install": "pnpm exec playwright install chromium --with-deps",
    "prepublish": "pnpm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate": "pnpx shadcn@2.4.1 add"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.1.0",
    "react-dom": "^18.0.0 || ^19.1.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@solana-foundation/ms-tools-config": "workspace:^",
    "@storybook/csf": "^0.1.13",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/theming": "^8.6.14",
    "@tailwindcss/postcss": "^4.1.5",
    "@testing-library/dom": "^10.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dom": "link:@types/@testing-library/dom",
    "lucide-react": "^0.511.0",
    "next-themes": "^0.4.6",
    "postcss": "^8.4.31",
    "react-hook-form": "^7.56.4",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.5",
    "vaul": "^1.1.2",
    "zod": "^3.25.30"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@playwright/test": "^1.52.0",
    "@storybook/addon-docs": "^9.0.4",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-themes": "^8.6.12",
    "@storybook/addon-vitest": "^9.0.4",
    "@storybook/blocks": "^8.6.12",
    "@storybook/react": "^9.0.4",
    "@storybook/react-vite": "^9.0.4",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.2",
    "@vitest/browser": "3.2.1",
    "@vitest/coverage-v8": "3.2.1",
    "autoprefixer": "^10.4.17",
    "css-loader": "^7.1.2",
    "playwright": "^1.52.0",
    "postcss-loader": "^8.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "process": "^0.11.10",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "storybook": "^9.0.4",
    "style-loader": "^4.0.0",
    "tsup": "^8.2.4",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5.5.3",
    "vitest": "^3.1.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  }
}
